{
  "name": "spantree-upstart",
  "version": "0.0.4",
  "author": "spantree",
  "summary": "Puppet Upstart module",
  "license": "Apache-2.0",
  "source": "https://github.com/Spantree/puppet-upstart.git",
  "project_page": "https://github.com/Spantree/puppet-upstart",
