# New Relic System Monitor configuration file.
#
# This configuration file is under puppet control.
# Any changes will be squashed when the agent runs every 30 minutes.

license_key=<%= @license_key %>
loglevel=<%= @log_level %>
logfile=/var/log/newrelic/nrsysmond.log
proxy=<%= @proxy_host %>:<%= @proxy_port %>
pidfile=/var/run/newrelic/nrsysmond.pid
collector_host=collector.newrelic.com
disable_docker=true
